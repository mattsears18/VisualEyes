{
  "name": "visualeyes",
  "version": "0.0.1",
  "description": "A visualization and statistical analysis web application for eye tracking data",
  "license": "MIT",
  "repository": "mattsears18/VisualEyes",
  "scripts": {
    "start": "meteor --settings settings.json",
    "startMatt": "meteor --settings settingsMatt.json",
    "test": "TEST_WATCH=1 meteor test --driver-package meteortesting:mocha --port 3002",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage:unit": "BABEL_ENV=COVERAGE TEST_BROWSER_DRIVER=puppeteer COVERAGE=1 COVERAGE_OUT_HTML=1 COVERAGE_APP_FOLDER=$PWD/ meteor test --once --driver-package meteortesting:mocha",
    "coverage:watch": "BABEL_ENV=COVERAGE COVERAGE=1 COVERAGE_VERBOSE=1 COVERAGE_APP_FOLDER=$PWD/ TEST_WATCH=1 meteor test --driver-package meteortesting:mocha"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "babel": {
    "env": {
      "COVERAGE": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "keywords": [
    "convex",
    "hull",
    "convex hull",
    "eye tracking"
  ],
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "area-polygon": "^1.0.1",
    "aws-sdk": "^2.442.0",
    "bcrypt": "^3.0.6",
    "csvtojson": "^2.0.8",
    "datatables-buttons": "^1.0.3",
    "datatables.net": "^1.10.19",
    "datatables.net-bs": "^1.10.19",
    "datatables.net-bs4": "^1.10.19",
    "datatables.net-buttons": "^1.5.6",
    "datatables.net-buttons-bs": "^1.5.6",
    "datatables.net-dt": "^1.10.19",
    "extend": "^3.0.2",
    "faker": "^4.1.0",
    "file-saver": "^2.0.1",
    "genstats": "0.0.1",
    "hull.js": "git://github.com/mattsears18/hull.git#length-3-pointsets",
    "image-size": "^0.6.3",
    "jStat": "^1.7.1",
    "jquery": "^3.4.0",
    "js-call-r": "0.0.4",
    "json2csv": "^4.5.0",
    "jstat": "^1.7.1",
    "lodash": "^4.17.11",
    "luxon": "^0.5.8",
    "meteor-job": "^1.5.2",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.24.0",
    "os": "^0.1.1",
    "papaparse": "^4.6.3",
    "plotly.js-dist": "^1.47.3",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "regression": "^2.0.1",
    "select2": "^4.0",
    "select2-bootstrap-theme": "0.1.0-beta.10",
    "simpl-schema": "1.4.3",
    "stringify-object": "^3.3.0",
    "time-hulls": "file:../time-hulls",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^5.1.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.3",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.0.0",
    "puppeteer": "^1.15.0"
  }
}

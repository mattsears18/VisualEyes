{
  "env": {
    "meteor": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "plugins": ["meteor"],
  "extends": ["plugin:meteor/recommended", "@meteorjs/eslint-config-meteor"],
  "globals": {
    "Analyses": true,
    "Aois": true,
    "Datafiles": true,
    "Gazepoints": true,
    "Jobs": true,
    "Participants": true,
    "Stimuli": true,
    "Stimulusfiles": true,
    "Studies": true,
    "Users": true,
    "Variablefiles": true,
    "Variables": true,
    "Visits": true,
    "Schemas": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "no-use-before-define": "off"
  }
}
